<h2>Ingresa los datos de la direccion</h2>
  <div class="row">
    <form action="/admin/direcciones" method='post' class="col s12">
      <div class="row">
        <div class="input-field col s12 m6 l6">
          <input id="departamento" name="departamento" type="text" class="validate" required="">
          <label for="departamento">Departamento</label>
        </div>
        <div class="input-field col s12 m6 l6">
          <input id="ciudad" name="ciudad" type="text" class="validate" required="">
          <label for="ciudad">Ciudad</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s12 m6 l6">
          <input id="calle" name="calle" type="text" class="validate" required="">
          <label for="calle">Calle</label>
        </div>
        <div class="input-field col s12 m6 l6">
          <input name="numero" id="numero" type="text" class="validate" required="">
          <label for="numero">Numero</label>
        </div>
      </div>
      <div class="row center">
        <button class="btn waves-effect waves-light" type="submit" name="action">
          Enviar
          <i class="material-icons right">send</i>
        </button>
        <button class="btn waves-effect waves-light" type="" name="cancel" onclick="cancelDirAdd()">
          Cancelar
          <i class="material-icons right">backspace</i>
        </button>
      </div>
    </form>
  </div>
